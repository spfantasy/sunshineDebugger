{
  "name": "sunshinedebugger",
  "version": "1.0.0",
  "main": "build/main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "start": "NODE_ENV=dev electron .",
    "dist": "NODE_ENV=prod electron-builder --win portable --x64"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@dagrejs/dagre": "^1.1.2",
    "@vue-flow/background": "^1.3.0",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.36.0",
    "@vue-flow/minimap": "^1.5.0",
    "@vue-flow/node-resizer": "^1.4.0",
    "@vue-flow/node-toolbar": "^1.1.0",
    "axios": "^1.7.2",
    "codemirror": "^6.0.1",
    "express": "^4.19.2",
    "json5": "^2.2.3",
    "mysql2": "^3.10.1",
    "view-ui-plus": "^1.3.17",
    "vue": "^3.4.29",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.3.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "css-loader": "^7.1.2",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "file-loader": "^6.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^5.2.8",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.92.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.sunshine.debgguer",
    "productName": "SunshineDebugger",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraFiles": [
      "backend/**/*",
      "config/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "asar": true,
    "extraMetadata": {
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "win": {
      "target": ["portable"]
    },
    "portable": {
      "artifactName": "sunshineDebugger.exe"
    }
  }
}
